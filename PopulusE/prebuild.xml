<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="buildinfofile" name="Create build info file">
    <!--ANT 1.7 is required                                        -->
    <target name="buildinfofile">
    	<tstamp>
    	    <format property="STARTTIME" pattern="d-MMMM-yyyy hh:mm aa"/>
    	</tstamp>
    	<echo file="build/classes/edu/umn/ecology/populus/core/timestamp.txt" append="false" message="${STARTTIME}" />
    </target>
</project>
